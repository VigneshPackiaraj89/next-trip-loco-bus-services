{
  "name": "next-trip-loco-bus-services",
  "version": "1.0.0",
  "description": "page to show the loco bus services timing and routes",
  "scripts": {
    "build": "eslint && rm -rf dist lib views && webpack --env production --progress",
    "cypress-open": "sh cypress/start-cypress.sh case-open",
    "cypress-test": "sh cypress/start-cypress.sh",
    "deploy-backend": "tar -xf next-trip-loco-bus-services-*.tgz -C target && grunt artifactory:backend:publish",
    "deploy-frontend": "rm -f deploy/*.war && grunt artifactory:frontend:publish",
    "lint": "eslint --ext .jsx --ext .js src",
    "package-node": "rm -f next-trip-loco-bus-services-*.tgz && npm pack",
    "start": "webpack-dev-server --env development",
    "start-background": "if ! nc -z localhost 8081; then nohup npm start; fi",
    "start-node": "node server.js",
    "test": "jest --coverage",
    "go:jenkins": "npm run lint && npm run test && npm run build && npm run package-node && npm run cypress-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VigneshPackiaraj89/next-trip-loco-bus-services.git"
  },
  "author": "vignesh p p",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VigneshPackiaraj89/next-trip-loco-bus-services/issues"
  },
  "homepage": "https://github.com/VigneshPackiaraj89/next-trip-loco-bus-services#readme",
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-react": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.8.0",
    "babel-jest": "^27.4.5",
    "babel-loader": "^8.2.3",
    "css-loader": "^4.3.0",
    "consolidate": "^0.16.0",
    "cookie-parser": "^1.4.6",
    "copy-webpack-plugin": "^6.2.0",
    "cypress": "^9.1.1",
    "eslint": "^8.5.0",
    "eslint-plugin-react": "^7.27.1",
    "express": "^4.17.2",
    "file-loader": "^6.2.0",
    "grunt-artifactory-artifact": "^2.0.0-rc.0",
    "html-webpack-plugin": "^4.5.0",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "less-loader": "^7.0.1",
    "path": "^0.12.7",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.3",
    "whatwg-fetch": "^3.6.2"
  },
  "dependencies": {
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^4.2.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1",
    "style-loader": "2.0.0"
  }
}
